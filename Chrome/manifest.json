{
  "name": "LibProxy",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {"128": "icon128.png" },
  "description": "Automatically loads Publishers pages through Warwick Library Proxy",
  "author": "Keir Lawson",
  "homepage_url": "http://keir-l.github.io",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "LibProxy"
  },
  "permissions": [
    "activeTab", "https://link.springer.com/", "https://www.link.springer.com/", "https://www.journals.sagepub.com/", "https://www.cambridge.org/", "https://www.tandfonline.com/", "https://www.jstor.org/", "https://www.heinonline.org/", "https://www.academic.oup.com/"
  ]
}
